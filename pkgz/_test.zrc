
pkg\ _test () {

    __install__ () {
        mkdir -p _test
        @ log "__install__"
        @ load
    }

    __init__ () {
        @ log "__init__"
        @ setup
        # rm -rf _test
    }

    : wd "$PWD"/_test

    \ uninstall () {
        @ log "uninstall"
        @proto uninstall
    }

    \ setup () {
        @ log "setup"
    }

    \ load () {
        @ log "load"
        _test will-load
    }

    \ will-load () {
        @ log "will-load"
    }

    \ test-self () {
        echo 1
        pkg _test log "$: pkg _test log"

        echo 2
        _test log "$: _test log"

        echo 3
        @ log "$: @ log"

        echo 4
        # if [[ $PKG == _test ]] ; then echo YYY; else echo NNN; fi
        eval "$PKG log \"\$PKG log\""

        echo 5
        pkg $PKG log "$: pkg \$PKG log"

        echo 6
        local id=$PKG
        $id log "\$id log"

        echo 7
        $PKG log "\$PKG log"
    }

    \ log () {
        echo "_test : $argv // $PKG $PWD"
    }

}

